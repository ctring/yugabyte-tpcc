<?xml version="1.0"?>
<parameters>
    <enableGeoPartitionedWorkload>true</enableGeoPartitionedWorkload>
    <numberOfPartitions>3</numberOfPartitions>
    <tablespaces>
        <tablespace>
            <name>us-east-1</name>
            <storePartitions>true</storePartitions>
            <replicationFactor>1</replicationFactor>
            <placementPolicy>
                <placementBlock>
                    <cloud>aws</cloud>
                    <region>region-1</region>
                    <zone>region-1</zone>
                    <minReplicationFactor>1</minReplicationFactor>
                </placementBlock>
            </placementPolicy>
        </tablespace>
        <tablespace>
            <name>us-west-1</name>
            <storeItemTable>true</storeItemTable>
            <storePartitionedTables>true</storePartitionedTables>
            <storePartitions>true</storePartitions>
            <replicationFactor>1</replicationFactor>
            <placementPolicy>
                <placementBlock>
                    <cloud>aws</cloud>
                    <region>region-2</region>
                    <zone>region-2</zone>
                    <minReplicationFactor>1</minReplicationFactor>
                </placementBlock>
            </placementPolicy>
        </tablespace>
        <tablespace>
            <name>ap-northeast-1</name>
            <storePartitions>true</storePartitions>
            <replicationFactor>1</replicationFactor>
            <placementPolicy>
                <placementBlock>
                    <cloud>aws</cloud>
                    <region>region-3</region>
                    <zone>region-3</zone>
                    <minReplicationFactor>1</minReplicationFactor>
                </placementBlock>
            </placementPolicy>
        </tablespace>
    </tablespaces>
</parameters>
